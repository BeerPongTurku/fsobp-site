{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "indent": [
      2,
      2
    ],
    "linebreak-style": 0,
    "no-use-before-define": [
      2,
      {
        "variables": false,
        "functions": false
      }
    ],
    "comma-dangle": [
      2,
      0
    ],
    "semi": [
      0,
      "always"
    ],
    "object-shorthand": [
      2,
      "consistent"
    ],
    "no-console": 0,
    "prefer-template": 0,
    "array-callback-return": 1
  }
}
/* SEMI
        "arrays": 2,
        "objects": 2,
        "imports": 0,
        "exports": 0,
        "functions": 1
        */